    },
    fetchMapHelper: function(component,event, helper){
         var action = component.get("c.getMapData");
        action.setCallback(this, function(response) {
            var state = response.getState();
            if(state === "SUCCESS")
            {
                var mapDataList = response.getReturnValue();
                var mapDataArray = [];
               /* for(int i=0; i<mapDataList.length; i++){
                    mapDataArray.push({label: mapDataList[i],name: mapDataList[i]})
                }*/
                console.log(mapDataArray);
                component.set("v.mapList", mapDataArray);
            }
            else
            {
                console.log("State was: " + state);
            }
        });
        $A.enqueueAction(action);
	
    }
   
})
